        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Estados Boss

switch (global.estadoBoss)
{
    case "stop":
        scr_enterBoss();
    break;
    
    case "war":
        obj_tanque.activo = true;//activa movimiento de player
        
        switch (faseAtaque)
        {
        
            //fase cero --no Atack
            case 0:
                if(instance_exists(par_balaplayer))
                {
                    faseAtaque++;
                                
                    if (global.nombreBoss == "GhostNeon") path_start( path_BossNeon,5,2,0); else
                    if (global.nombreBoss == "Electro") path_start( path_BossElectro,5,2,0);                 
                }
                
            break;
            
            //primera fase -- init atack
            case 1:
                if(global.nombreBoss == "Hunter"){scr_MilitarFaseAtack1();}else
                if(global.nombreBoss == "Marino"){scr_MarinoFaseAtack1();}else
                if(global.nombreBoss == "Kristal"){scr_IceFaseAtack1();}else
                if(global.nombreBoss == "Vulcano"){scr_FireFaseAtack1();}//else
                //if(global.nombreBoss == "Magnus"){scr_MagnusFaseAtack1();}
                else{scr_faseAtack1();}//Ataque por defecto del primer boss
             
            break;
    
             //segunda fase
            case 2:
                //speed = 3;
                if (global.nombreBoss == "Hunter"){scr_MilitarFaseAtack2();}else
                if(global.nombreBoss == "Marino"){scr_MarinoFaseAtack2();}else
                if(global.nombreBoss == "Kristal"){scr_IceFaseAtack2();}else
                if(global.nombreBoss == "Vulcano"){scr_FireFaseAtack2();}//else
                //if(global.nombreBoss == "Magnus"){scr_MagnusFaseAtack2();}
                else{
                    if (instance_number( balaBoss)&lt;6 and place_free (x , y +20))
                        instance_create( x, y + 20, balaBoss);
                        
                    if global.saludBoss &lt;= 50 faseAtaque++;
                }
                
             
            break;
            
            //tercera fase 
            case 3:
                if (global.nombreBoss == "Electro"){scr_ElectroFaseAtack3();}else
                if (global.nombreBoss == "Hunter"){scr_MilitarFaseAtack3();}else
                if (global.nombreBoss == "Marino"){scr_MarinoFaseAtack3();}else
                if(global.nombreBoss == "Kristal"){scr_IceFaseAtack3();}else
                if(global.nombreBoss == "Vulcano"){scr_FireFaseAtack3();}//else
                //if(global.nombreBoss == "Magnus"){scr_MagnusFaseAtack3();}
                else{
                    if (instance_number( balaBoss2)&lt;=4 and place_free( x, y + 15))
                        instance_create( x, y + 2, balaBoss2);
                        
                    if global.saludBoss &lt;= 30 faseAtaque++;
                }
                
            
            break;
            
            //cuarta fase
            case 4: 
                //speed = 3;
                if(global.nombreBoss == "GhostNeon"){scr_faseAtack2();}else
                if(global.nombreBoss == "Electro"){scr_ElectroFaseAtack4();}else
                if(global.nombreBoss == "Hunter"){scr_MilitarFaseAtack4();}else
                if(global.nombreBoss == "Marino"){scr_MarinoFaseAtack4();}else
                if(global.nombreBoss == "Kristal"){scr_IceFaseAtack4();}else
                if(global.nombreBoss == "Vulcano"){scr_FireFaseAtack4();}//else
                //if(global.nombreBoss == "Magnus"){scr_MagnusFaseAtack4();}
                else
                {
                    //Esta parte es para el primer boss
                        
                    //if (distance_to_object( obj_hurricane) &gt; 20)
                     //   instance_create(x,y + 20, obj_hurricane);  
                       
                    if ( !instance_exists( obj_hurricane)) instance_create(x,y + 20,obj_hurricane)
                      
                    if global.saludBoss &lt;= 0 faseAtaque++;
                }
                
            break;
            
            case 5://Vencido
                if(global.nombreBoss == "Vulcano"){
                    rotacion +=10;
                    image_angle = lerp(image_angle,rotacion,0.3);
                    vspeed = 10;
                }//else
                //if (global.nombreBoss == "Magnus"){
                   // scr_MagnusFaseAtack5();
                //}
                scr_vencido();
                //if ( obj_control.alarm[1]&lt;= room_speed * 2){ instance_destroy();}

            break;
            
            //case 6://Solo aplica para boss Magnus
               // scr_vencido();
            //break;
        }
    break;
}
   

</string>
