      if ( opcionPausa &lt; array_length_1d( arrPausa) -1) opcionPausa++;
}

//if(Pausa == true){   
if (keyboard_check_pressed( vk_enter))
{
   switch(opcionPausa)
        {
          case 0://Continuar
            if sprite_exists(imagenpausa)sprite_delete(imagenpausa);
            imagenpausa = -1;
            instance_activate_all();
            Pausa = false;
            instance_destroy();

          break; 
          
          case 1://volver a menu
            //room_goto( rom_menu);
            game_restart();
            with( obj_base){ instance_destroy();}//destruimos la instance base para que no aparezca en el menu.
            with( obj_base_superior){ instance_destroy();}//destruimos instancia base para que no aparezca en el menu.
            with( obj_control){ instance_destroy();}//destruimos instancia de objeto control.
            instance_destroy();//destruimos el objeto pausaMapa
          break;
          
          case 2://salir del juego
            game_end();
          break;
        }
}
