            <kind>1</kind>
            <string>///Salto, movimiento y disparo

if ( activo == true)
{
    //movimiento horizontal
    key_right = keyboard_check( vk_right);
    key_left = -keyboard_check( vk_left);
    move = key_left + key_right;
    movehor = move * movespeed;
    
    hspeed = +movehor;
    //cambio de sprite
    if (movehor !=0)
    { sprite_index = spr_tankMove;}// spr_tanquemove;}
    else{ sprite_index = spr_tankStop;} //spr_tanque_stop;}
    //Fin movimiento horizontal
    
    
    //Disparo
    if( distance_to_object( obj_Ballfire)&gt;50){
        if( keyboard_check( ord("A")))
            {
             instance_create(x, y, obj_Ballfire);
             obj_Ballfire.vspeed =-10;
            }
    }
    //Fin disparo
    
    
    
    //Gravedad
    if ( vspeed &lt;20) &amp;&amp; !place_meeting(x ,y +1, obj_base)
       {vspeed +=gravedad;}
    
        //gravity_direction = 270;
        //gravity = 0.5;
    //Fin Gravedad
    
    //Velocidad vertical
    if place_meeting(x,y+vspeed, obj_base)
    {
       while ( !place_meeting(x, y + sign( vspeed), obj_base))
        {
          y += sign(vspeed);
        };
      vspeed = 0;
    }
    //Fin velocidad vertical
    
    
    //Salto
    if ( place_meeting( x, y+1, obj_base))
    {
       if ( keyboard_check_pressed( vk_up))
       {
       vspeed = -12;
       }
    }
    //Fin salto
    

}
else { speed = 0;}//cancelamos velocidad
